{
  "version": 2,
  "headers": [
    {
      "source": "/videos/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Content-Type",
          "value": "video/mp4"
        },
        {
          "key": "Accept-Ranges",
          "value": "bytes"
        }
      ]
    }
  ],
  "builds": [
    {
      "src": "api/planyourjourney.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/charging_cost.py",
      "use": "@vercel/python"
    },
    {
      "src": "**/*.html",
      "use": "@vercel/static"
    },
    {
      "src": "**/*.css",
      "use": "@vercel/static"
    },
    {
      "src": "**/*.js",
      "use": "@vercel/static"
    },
    {
      "src": "images/**/*",
      "use": "@vercel/static"
    },
    {
      "src": "icons/**/*",
      "use": "@vercel/static"
    },
    {
      "src": "videos/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/proxy-api/(.*)",
      "dest": "api/planyourjourney.py"
    },
    {
      "src": "/estimate-charging-cost/(.*)",
      "dest": "api/charging_cost.py"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
